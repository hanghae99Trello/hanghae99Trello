{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "docker_trello",
      "image": "gyungtaek/gyungtaek:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 1234,
          "containerPort": 1234
        }
      ],
      "links": ["redis"]
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 6380,
          "containerPort": 6379
        }
      ]
    }
  ]
}
